<!DOCTYPE html>
<html lang="pt-br">
<head>
    <style>
        tr, th, td{
            border: 1px solid black;
            padding: 5px;
        }
    </style>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tabela</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <% header.forEach(col => { %>
                    <th><%= col %></th>
                <% }) %>
            </tr>
        </thead>
        <tbody>
            <% content.forEach(row => { %>
                <tr>
                    <% row.forEach(col => { %>
                        <td><%= col %></td>
                    <% }) %>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>